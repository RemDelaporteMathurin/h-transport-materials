	number = {4},
	urldate = {2022-02-04},
	journal = {Fusion Science and Technology},
	author = {Zhang, Dongxun and Liu, Wei and Liu, Wenguan},
	year = {2020},
	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/15361055.2020.1725368},
	keywords = {fluoride salt, Hastelloy N alloy, permeation, surface oxidation},
	pages = {543--552},
