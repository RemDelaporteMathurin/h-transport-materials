	author = {Liang, Chuan-hui and Wang, Dongping and Jin, Wei and Lou, Yuanfu and Wang, Wei and Ye, Xiaoqiu and Chen, Chang-an and Liu, Kezhao and Xu, Haiyan and Wang, Xiaoying and Kleyn, Aart W.},
