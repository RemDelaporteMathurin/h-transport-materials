	author = {Young, G. A. and Scully, J. R.},
	month = nov,
	year = {1998},
	pages = {6337--6349},
	file = {ScienceDirect Snapshot:C\:\\Users\\remidm\\Zotero\\storage\\ARBAIVV8\\S1359645498003334.html:text/html},
